			<div id="loaded-content">
				<!-- The loaded HTML content will be displayed here. -->
			</div>



			<!-- Load Elements by events -->
			<script>
			var derectionfornewuser = document.getElementById("derectionfornewuser");
			 function joininmatch() {
				 derectionfornewuser.style.left= "200%";
			    fetch("matchentryoptions.jsp")
			        .then(response => response.text())
			        .then(data => {
			            document.getElementById("loaded-content").innerHTML = data;
			        })
			        .catch(error => {
			            console.error("Error loading HTML content:", error);
			        });
			};

			
			</script>